{
  "name": "@odjs/classes",
  "version": "0.1.0",
  "description": "Classname management for @odjs/dom",
  "keywords": [
    "odjs",
    "dom",
    "class",
    "classes",
    "classname",
    "classnames"
  ],
  "main": "dist/classes.cjs.js",
  "module": "dist/classes.es.js",
  "browser": "dist/classes.umd.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "bundlib": {
    "min": "browser",
    "equals": true
  },
  "scripts": {
    "clean": "rm -rf node_modules/.cache/bundlib node_modules/.cache/jest dist types coverage",
    "lint": "tslint src/**/*.ts",
    "dev": "bundlib -dw",
    "build": "npm run lint && bundlib -d",
    "test": "jest --color -w 3",
    "preversion": "npm run clean && npm run build && npm test"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "bundlib": "^0.12.0",
    "chokidar": "3.0.2",
    "eslint": "^6.1.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "files": [
    "dist",
    "types"
  ],
  "author": {
    "name": "Manuel Fernandez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/odjs/classes.git"
  },
  "bugs": {
    "url": "https://github.com/odjs/classes/issues"
  },
  "homepage": "https://github.com/odjs/classes#readme",
  "license": "MIT"
}
