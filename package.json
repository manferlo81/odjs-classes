{
  "name": "@odjs/classes",
  "version": "1.1.0",
  "description": "Classname management for @odjs/dom",
  "keywords": [
    "odjs",
    "dom",
    "class",
    "classes",
    "classname",
    "classnames"
  ],
  "main": "dist/classes.cjs.js",
  "module": "dist/classes.es.js",
  "browser": "dist/classes.umd.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "bundlib": {
    "min": "browser",
    "equals": true
  },
  "scripts": {
    "clean": "rm -rf node_modules/.cache/bundlib node_modules/.cache/jest dist types coverage",
    "lint": "eslint src/** test/**",
    "dev": "bundlib -dw",
    "build": "npm run lint && bundlib -d",
    "test": "jest --color -w 3",
    "preversion": "npm run clean && npm run build && npm test",
    "release": "standard-version"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "bundlib": "^0.13.0",
    "chokidar": "^3.1.1",
    "eslint": "^6.4.0",
    "jest": "^24.9.0",
    "standard-version": "^7.0.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  },
  "files": [
    "dist",
    "types"
  ],
  "author": {
    "name": "Manuel Fernandez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/odjs/classes.git"
  },
  "bugs": {
    "url": "https://github.com/odjs/classes/issues"
  },
  "homepage": "https://github.com/odjs/classes#readme",
  "license": "MIT"
}
